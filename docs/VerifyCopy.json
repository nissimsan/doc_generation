{
    "schema": {
        "model": "VerifyCopy",
        "type": "object",
        "properties": {
            "verifyCopy": {
                "$ref": "#/definitions/VerifyCopy"
            }
        },
        "definitions": {
            "Address": {
                "type": "object",
                "properties": {
                    "street": {
                        "description": "A name, expressed as text, of a street or thoroughfare.",
                        "type": "string"
                    },
                    "city": {
                        "description": "The name, expressed as text, of the city, town or village of this address.",
                        "type": "string"
                    },
                    "state": {
                        "description": "The name, expressed as text, of the state of this address.",
                        "type": "string"
                    },
                    "country": {
                        "description": "A unique country identifier for this party.",
                        "type": "string",
                        "enum": []
                    },
                    "postcode": {
                        "description": "A code specifying the postcode of the address.",
                        "type": "string"
                    }
                },
                "required": [
                    "street",
                    "city",
                    "state",
                    "country"
                ]
            },
            "BillOfLading": {
                "type": "object",
                "properties": {
                    "negotiable": {
                        "description": "Indicator, whether or not the bill of lading is negotiable. ",
                        "type": "boolean"
                    },
                    "freightedAtAd": {
                        "description": "Freighted at ad indicator. ",
                        "type": "boolean"
                    },
                    "certifiedMember": {
                        "description": "Certified member. ",
                        "type": "string"
                    },
                    "dateOfAcceptance": {
                        "description": "Acceptance date of the bill of lading. ",
                        "type": "string",
                        "format": "date-time"
                    },
                    "declarationOfInterest": {
                        "description": "Declaration of interest indicator. ",
                        "type": "boolean"
                    },
                    "eBL": {
                        "description": "Indication of this being an electronic bill of lading. ",
                        "type": "boolean"
                    },
                    "witness": {
                        "description": "In witness whereof this MTD has been signed",
                        "type": "string"
                    }
                },
                "allOf": [
                    {
                        "$ref": "#/definitions/TransportDocument"
                    },
                    {
                        "type": "object"
                    }
                ]
            },
            "CargoInformation": {
                "type": "object",
                "properties": {
                    "text": {
                        "description": "Textual cargo information.",
                        "type": "string"
                    },
                    "type": {
                        "description": "Type of cargo information. ",
                        "type": "string",
                        "enum": []
                    }
                }
            },
            "Carrier": {
                "type": "object",
                "properties": {
                    "carrierCode": {
                        "description": "SCAC code.",
                        "type": "string"
                    },
                    "name": {
                        "description": "Carrier name. ",
                        "type": "string"
                    },
                    "taxId": {
                        "description": "Carrier''s tax identification. ",
                        "type": "string"
                    },
                    "contactDetails": {
                        "$ref": "#/definitions/Communication"
                    },
                    "address": {
                        "$ref": "#/definitions/Address"
                    }
                },
                "required": [
                    "carrierCode",
                    "name"
                ]
            },
            "Charges": {
                "type": "object",
                "properties": {
                    "chargeCode": {
                        "description": "The unique identifier for this logistics service charge.",
                        "type": "string",
                        "enum": []
                    },
                    "chargeText": {
                        "description": "A textual description of this logistics service charge.",
                        "type": "string"
                    },
                    "rate": {
                        "description": "Per unit rate. ",
                        "type": "number"
                    },
                    "calculationBasis": {
                        "description": "The code specifying a basis on which this service charge is to be calculated such as by volume or per unit.",
                        "type": "string"
                    },
                    "currency": {
                        "description": "Charge currency. ",
                        "type": "string"
                    },
                    "paymentTerm": {
                        "description": "Charge payment term.",
                        "type": "string",
                        "enum": []
                    },
                    "amount": {
                        "description": "A monetary value applied to this logistics service charge.",
                        "type": "number"
                    },
                    "freightPayableAt": {
                        "description": "Charge receiver. ",
                        "type": "string"
                    },
                    "invoicePartyRef": {
                        "description": "Reference to the invoice party. ",
                        "type": "string"
                    },
                    "rateReference": {
                        "description": "Reference on the charge rate.",
                        "type": "string"
                    },
                    "invoicePartyName": {
                        "description": "missing description",
                        "type": "string"
                    }
                }
            },
            "Communication": {
                "type": "object",
                "properties": {
                    "telephone": {
                        "description": "Telephone number.",
                        "type": "string"
                    },
                    "fax": {
                        "description": "Fax number. ",
                        "type": "string"
                    },
                    "email": {
                        "description": "Email address.",
                        "type": "string"
                    },
                    "contactName": {
                        "description": "Name of contact person. ",
                        "type": "string"
                    }
                }
            },
            "Consignee": {
                "type": "object",
                "properties": {},
                "allOf": [
                    {
                        "$ref": "#/definitions/Party"
                    },
                    {
                        "type": "object"
                    }
                ]
            },
            "Consignment": {
                "type": "object",
                "properties": {
                    "departureDate": {
                        "description": "Date and time of departure. ",
                        "type": "string",
                        "format": "date-time"
                    },
                    "serviceMode": {
                        "allOf": [
                            {
                                "description": "The scope of the consignment, such as CY-CY or SD-SD."
                            },
                            {
                                "$ref": "#/definitions/Text"
                            },
                            {
                                "type": "object"
                            }
                        ]
                    },
                    "transportEquipmentQuantity": {
                        "description": "A number of pieces of transport equipment, such as containers or similar unit load devices, in this consignment.",
                        "type": "number"
                    },
                    "mainCarriageTransportMovement": {
                        "items": {
                            "$ref": "#/definitions/TransportMovement"
                        },
                        "type": "array"
                    },
                    "charges": {
                        "items": {
                            "$ref": "#/definitions/Charges"
                        },
                        "type": "array",
                        "minItems": 1
                    },
                    "carrier": {
                        "$ref": "#/definitions/Carrier"
                    },
                    "consignmentItems": {
                        "items": {
                            "$ref": "#/definitions/ConsignmentItem"
                        },
                        "type": "array",
                        "minItems": 1
                    },
                    "placeOfReceipt": {
                        "$ref": "#/definitions/Location"
                    },
                    "placeOfDelivery": {
                        "$ref": "#/definitions/Location"
                    },
                    "portOfLoading": {
                        "$ref": "#/definitions/Location"
                    },
                    "portOfDischarge": {
                        "$ref": "#/definitions/Location"
                    },
                    "placeOfPrecarriage": {
                        "$ref": "#/definitions/Location"
                    },
                    "otherParties": {
                        "items": {
                            "$ref": "#/definitions/OtherParty"
                        },
                        "type": "array"
                    },
                    "notifyParties": {
                        "items": {
                            "$ref": "#/definitions/NotifyParty"
                        },
                        "type": "array"
                    },
                    "consignor": {
                        "$ref": "#/definitions/Consignor"
                    },
                    "consignee": {
                        "$ref": "#/definitions/Consignee"
                    },
                    "cargoInformation": {
                        "items": {
                            "$ref": "#/definitions/CargoInformation"
                        },
                        "type": "array"
                    },
                    "utilizedTransportEquipment": {
                        "items": {
                            "$ref": "#/definitions/TransportEquipment"
                        },
                        "type": "array"
                    }
                },
                "required": [
                    "charges",
                    "carrier",
                    "consignmentItems",
                    "consignor",
                    "consignee"
                ]
            },
            "ConsignmentItem": {
                "type": "object",
                "properties": {
                    "sequence": {
                        "description": "The sequence number for this consignment item.",
                        "type": "number"
                    },
                    "information": {
                        "description": "Cargo description. Information, expressed as text, for this consignment item.",
                        "type": "string"
                    },
                    "codeType": {
                        "description": "The cargo code type of consignment item. \nhttps://service.unece.org/trade/untdid/d00a/tred/tred7143.htm",
                        "type": "string",
                        "enum": []
                    },
                    "codeValue": {
                        "description": "A code specifying a type of consignment item.",
                        "type": "string"
                    },
                    "transportPackages": {
                        "$ref": "#/definitions/TransportPackage"
                    },
                    "grossWeight": {
                        "items": {
                            "$ref": "#/definitions/Weight"
                        },
                        "type": "array"
                    },
                    "grossVolume": {
                        "items": {
                            "$ref": "#/definitions/Volume"
                        },
                        "type": "array"
                    },
                    "equipmentStuffing": {
                        "items": {
                            "$ref": "#/definitions/Stuffing"
                        },
                        "type": "array"
                    }
                }
            },
            "Consignor": {
                "type": "object",
                "properties": {},
                "allOf": [
                    {
                        "$ref": "#/definitions/Party"
                    },
                    {
                        "type": "object"
                    }
                ]
            },
            "Document": {
                "type": "object",
                "properties": {
                    "originatorName": {
                        "description": "The proper name (human readable) of the entity that published this event.",
                        "type": "string"
                    },
                    "originatorId": {
                        "description": "The party identification code (i.e. SCAC) of the organization publishing the event.",
                        "type": "string"
                    },
                    "issue": {
                        "description": "A date, time, date time or other date time value for an issuance of this document.",
                        "type": "string",
                        "format": "date-time"
                    },
                    "originalIssued": {
                        "description": "The number of originals issued for this document.",
                        "type": "number"
                    },
                    "copyIssued": {
                        "description": "The number of copies issued for this document.",
                        "type": "number"
                    },
                    "placeOfIssue": {
                        "$ref": "#/definitions/Location"
                    },
                    "references": {
                        "items": {
                            "$ref": "#/definitions/Reference"
                        },
                        "type": "array"
                    }
                }
            },
            "Location": {
                "type": "object",
                "properties": {
                    "name": {
                        "description": "A name, expressed as text, of this logistics related location.\nCorresponding EDIFACT element: 3225 - Location name code.",
                        "type": "string"
                    },
                    "UNLocode": {
                        "description": "United Nations Location Code (UNLOCODE) unique identifier for logistics related location. ",
                        "type": "string"
                    },
                    "printedLocation": {
                        "description": "The location exactly as printed on the document.",
                        "type": "string"
                    }
                }
            },
            "NotifyParty": {
                "type": "object",
                "properties": {},
                "allOf": [
                    {
                        "$ref": "#/definitions/Party"
                    },
                    {
                        "type": "object"
                    }
                ]
            },
            "OtherParty": {
                "type": "object",
                "properties": {
                    "partyRole": {
                        "description": "Type of party. ",
                        "type": "string",
                        "enum": []
                    }
                },
                "allOf": [
                    {
                        "$ref": "#/definitions/Party"
                    },
                    {
                        "type": "object"
                    }
                ]
            },
            "Party": {
                "type": "object",
                "properties": {
                    "name": {
                        "description": "A name, expressed as text, for this party.",
                        "type": "string"
                    },
                    "printedParty": {
                        "description": "The party name and address exactly as printed on the document.",
                        "type": "string"
                    },
                    "registeredForVAT": {
                        "description": "VAT registration indicator. ",
                        "type": "boolean"
                    },
                    "taxReference": {
                        "description": "Tax identification of the party.",
                        "type": "string"
                    },
                    "partyRef": {
                        "description": "Reference which represents the party.",
                        "type": "string"
                    },
                    "organizationId": {
                        "description": "Unique identifier of the party. ",
                        "type": "string"
                    },
                    "address": {
                        "$ref": "#/definitions/Address"
                    },
                    "communicationDetails": {
                        "$ref": "#/definitions/Communication"
                    }
                }
            },
            "Reference": {
                "type": "object",
                "properties": {
                    "type": {
                        "description": "Type of reference. ",
                        "type": "string",
                        "enum": []
                    },
                    "value": {
                        "description": "Reference value. ",
                        "type": "string"
                    }
                }
            },
            "Seal": {
                "type": "object",
                "properties": {
                    "type": {
                        "description": "Description specifying the type of logistics seal.",
                        "type": "string"
                    },
                    "identifier": {
                        "description": "A unique identifier for this logistics seal.",
                        "type": "string"
                    }
                }
            },
            "Stuffing": {
                "type": "object",
                "properties": {
                    "equipmentNumber": {
                        "description": "Unique identifier of the transport equipment in which cargo is stuffed. ",
                        "type": "string"
                    },
                    "stuffedGrossVolume": {
                        "items": {
                            "$ref": "#/definitions/Volume"
                        },
                        "type": "array"
                    },
                    "stuffedGrossWeight": {
                        "items": {
                            "$ref": "#/definitions/Weight"
                        },
                        "type": "array"
                    },
                    "stuffedTransportPackages": {
                        "items": {
                            "$ref": "#/definitions/TransportPackage"
                        },
                        "type": "array"
                    }
                }
            },
            "TransportDocument": {
                "type": "object",
                "properties": {
                    "shippedOnBoardDate": {
                        "description": "Date when the cargo was shipped on board. ",
                        "type": "string",
                        "format": "date-time"
                    },
                    "billOfLadingNumber": {
                        "description": "Bill of lading number of the transport document. ",
                        "type": "string"
                    },
                    "bookingNumber": {
                        "items": {
                            "description": "Booking number(s). ",
                            "type": "string"
                        },
                        "type": "array"
                    },
                    "information": {
                        "items": {
                            "$ref": "#/definitions/TransportDocumentInformation"
                        },
                        "type": "array"
                    }
                },
                "allOf": [
                    {
                        "$ref": "#/definitions/Document"
                    },
                    {
                        "type": "object"
                    },
                    {
                        "$ref": "#/definitions/Consignment"
                    },
                    {
                        "type": "object"
                    }
                ],
                "required": [
                    "billOfLadingNumber"
                ]
            },
            "TransportDocumentInformation": {
                "type": "object",
                "properties": {
                    "text": {
                        "description": "Textual transport document information. ",
                        "type": "string"
                    },
                    "type": {
                        "description": "Type of transport document information. ",
                        "type": "string",
                        "enum": []
                    }
                }
            },
            "TransportEquipment": {
                "type": "object",
                "properties": {
                    "equipmentNumber": {
                        "allOf": [
                            {
                                "description": "Unique identifier for this piece of transport equipment, printed physically on the equipment."
                            },
                            {
                                "$ref": "#/definitions/Identifier"
                            },
                            {
                                "type": "object"
                            }
                        ]
                    },
                    "equipmentType": {
                        "description": "ISO 6346 size and type code (for example \"45G1\")",
                        "type": "string"
                    },
                    "printedEquipmentType": {
                        "description": "The container size and type, as printed on the transport document (for example \"40 DRY 9''6\").",
                        "type": "string"
                    },
                    "code": {
                        "items": {
                            "description": "The harmonized system code specifying a type of loaded consignment item(s).",
                            "type": "string"
                        },
                        "type": "array"
                    },
                    "seals": {
                        "items": {
                            "$ref": "#/definitions/Seal"
                        },
                        "type": "array"
                    },
                    "equipmentGrossWeight": {
                        "$ref": "#/definitions/Weight"
                    }
                },
                "required": [
                    "equipmentNumber"
                ]
            },
            "TransportMeans": {
                "type": "object",
                "properties": {
                    "identification": {
                        "description": "An identifier of this logistics means of transport, such as the International Maritime Organization number of a vessel.",
                        "type": "string"
                    },
                    "name": {
                        "description": "The name, expressed as text, of this logistics means of transport such as the vessel name.",
                        "type": "string"
                    }
                }
            },
            "TransportMovement": {
                "type": "object",
                "properties": {
                    "identification": {
                        "description": "To identify a journey of a means of transport, for example voyage number, flight number, trip number.",
                        "type": "string"
                    },
                    "usedTransportMeans": {
                        "$ref": "#/definitions/TransportMeans"
                    }
                }
            },
            "TransportPackage": {
                "type": "object",
                "properties": {
                    "quantity": {
                        "description": "The number of logistics packages at this level.",
                        "type": "number"
                    },
                    "type": {
                        "description": "A type, expressed as text, of this logistics package.",
                        "type": "string"
                    },
                    "typeCode": {
                        "description": "A code specifying the type of logistics package.",
                        "type": "string"
                    },
                    "shippingMarks": {
                        "items": {
                            "description": "Physical markings or labels on individual packages or transport units for shipping purposes.",
                            "type": "string"
                        },
                        "type": "array"
                    }
                }
            },
            "VerifyCopy": {
                "type": "object",
                "properties": {},
                "allOf": [
                    {
                        "$ref": "#/definitions/BillOfLading"
                    },
                    {
                        "type": "object"
                    }
                ]
            },
            "Volume": {
                "type": "object",
                "properties": {
                    "value": {
                        "description": "Numeric measurement value. ",
                        "type": "number"
                    },
                    "unit": {
                        "description": "Unit used for measurements. ",
                        "type": "string",
                        "enum": []
                    }
                }
            },
            "Weight": {
                "type": "object",
                "properties": {
                    "value": {
                        "allOf": [
                            {
                                "description": "Numeric weight value. "
                            },
                            {
                                "$ref": "#/definitions/Numeric"
                            },
                            {
                                "type": "object"
                            }
                        ]
                    },
                    "unit": {
                        "description": "Unit used for weight measurement.",
                        "type": "string",
                        "enum": []
                    }
                }
            },
            "Text": {
                "type": "object",
                "properties": {}
            },
            "Indicator": {
                "type": "object",
                "properties": {}
            },
            "DateTime": {
                "type": "object",
                "properties": {}
            },
            "Numeric": {
                "type": "object",
                "properties": {}
            },
            "Identifier": {
                "type": "object",
                "properties": {}
            }
        }
    },
    "layout": [
        {
            "widget": "message",
            "message": "<h1>verifyCopy</h1>"
        },
        {
            "key": "negotiable"
        },
        {
            "key": "freightedAtAd"
        },
        {
            "key": "certifiedMember"
        },
        {
            "key": "dateOfAcceptance"
        },
        {
            "key": "declarationOfInterest"
        },
        {
            "key": "eBL"
        },
        {
            "key": "witness"
        },
        {
            "key": "shippedOnBoardDate"
        },
        {
            "key": "billOfLadingNumber",
            "required": true
        },
        {
            "key": "bookingNumber"
        },
        {
            "key": "originatorName"
        },
        {
            "key": "originatorId"
        },
        {
            "key": "issue"
        },
        {
            "key": "originalIssued"
        },
        {
            "key": "copyIssued"
        },
        {
            "key": "departureDate"
        },
        {
            "key": "serviceMode",
            "type": "section",
            "items": [
                {
                    "key": "Text"
                }
            ]
        },
        {
            "key": "transportEquipmentQuantity"
        }
    ]
}